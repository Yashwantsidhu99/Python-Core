   "source": [
    "# Pallindrome and Length is Greater than 2 or Not\n",
    "c=0\n",
    "d=[\"abc\",\"xyz\",\"aba\",\"1221\"]\n",
    "for i in d:\n",
    "    if(len(i)>=2 and i==i[::-1]):\n",
    "        c=c+1\n",
    "        print(i)\n",
    "print(c)        "
   ]
  }
